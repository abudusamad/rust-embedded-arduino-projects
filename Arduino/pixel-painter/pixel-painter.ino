// Nothing really worth mentioning here
void setup() {
#if defined (__AVR_ATtiny85__)
  if (F_CPU == 16000000) clock_prescale_set(clock_div_1);
#endif

  pixels.begin();

   display.begin(SSD1306_SWITCHCAPVCC, 0x3C);
   display.display();

   display.setTextSize(2);
  display.setTextColor(WHITE);
  display.setCursor(10,0);
  display.clearDisplay();
  display.println("Running");
  display.display();
}

void loop() {

  // Turn off all pixels
  for (int i = 0; i < NUMPIXELS; i++) {
    pixels.setPixelColor(i, pixels.Color(0, 0, 0));
  }
  pixels.show();
  delay(3000);

  // Ready, set, go!
  for (int i = 0; i < COUNTDOWN_SECS; i++) {
    // Set both top and bottom (to allow easier alignment in camera)
    pixels.setPixelColor(i, pixels.Color(100, 0, 0));
    pixels.setPixelColor(NUMPIXELS - 1 - i, pixels.Color(100, 0, 0));
    pixels.show();
    delay(1000);
  }

  // Iterate over the columns
  for (int x = 0; x < IMAGE_COLUMNS; x++) {

    // Interate and render all rows in that column
    for (int y = 0; y < NUMPIXELS; y++) {

      // Read the color from PROGMEM as a long
      long color = pgm_read_dword(&(image[x][y]));

      // Do some computer science stuff to the color
      // (This is bitshifting, check it out - it's a great read. Seriously)
      int r = (color & 0xFF0000) >> 16;
      int g = (color & 0xFF00) >> 8;
      int b = (color & 0xFF);

      // Finally set the color of that particular pixel
      pixels.setPixelColor(y, pixels.Color(r, g, b));
    }
    
    pixels.show();
    
    delay(COLUMN_DELAY);
  }
}